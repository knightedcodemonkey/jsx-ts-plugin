{
  "name": "@knighted/jsx-ts-plugin",
  "version": "0.1.2",
  "description": "TypeScript language service plugin for @knighted/jsx tagged template diagnostics.",
  "main": "plugin.cjs",
  "types": "dist/cjs/jsx-ts-plugin.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/esm/jsx-ts-plugin.mjs",
      "require": "./plugin.cjs",
      "default": "./plugin.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "duel --dirs",
    "clean": "rm -rf dist",
    "prepack": "npm run build"
  },
  "files": [
    "dist",
    "plugin.cjs"
  ],
  "keywords": [
    "typescript",
    "language-service",
    "plugin",
    "jsx",
    "templates",
    "diagnostics"
  ],
  "author": "KCM <knightedcodemonkey@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knightedcodemonkey/jsx-ts-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/knightedcodemonkey/jsx-ts-plugin/issues"
  },
  "peerDependencies": {
    "typescript": ">=5.4.0"
  },
  "engines": {
    "node": ">=22"
  }
}
